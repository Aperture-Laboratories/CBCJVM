package cbccore.motors;

public class Motor {
	public void motor (int percent);
	public int clear_motor_position_counter();
	public int move_at_velocity(int velocity);
	public int mav(int velocity);
	public int moveToPosition(int speed, int goal_pos);
	public int mtp(int speed, int goal_pos);
	public int moveRelativePosition(int speed, int delta_pos);
	public int mrp(int speed, int delta_pos);
	public void setPidGains(int p, int i, int d, int pd, int id, int dd);
	public int freeze();
	public int getDone();
	public int getPositionCounter();
	public void blockMotorDone();
	public void bmd();
	public int setpwm(int pwm);
	public int getpwm();
	public void fd();
	public void bk();
	public void off();
}
