	protected CBCSimulator cbc;
	
	public SimulatedSound(CBCSimulator c) {
		cbc = c;
	}
	
	public void tone(int frequency, int duration) { /* makes a sound at frequency for duration ms */
		
	}
	
	public void beep() { /* make a beep */
		
	}
}
