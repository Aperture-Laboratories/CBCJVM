package cbccore.sensors.camera;

import cbccore.DeviceSingleton;

public class Camera {
	private cbccore.low.Camera lowCamera = DeviceSingleton.getInstance().getLowCameraController();
	public Camera() {
		lowCamera.track_init();
	}      
	public void update() {
		lowCamera.track_update();
	}
	public native int track_is_new_data_available(); 
	public native int track_get_frame(); 
	public native int track_capture_time();
	public native int track_previous_capture_time();
}
